{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "react/jsx-no-useless-fragment": 0,
        "react/react-in-jsx-scope": 0,
        "react/prop-types": 0,
        "import/no-named-as-default": 0,
        "import/no-extraneous-dependencies": 0,
        "jsx-a11y/control-has-associated-label": 0
    }
}
